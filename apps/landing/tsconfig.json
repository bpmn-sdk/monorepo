{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"declaration": false,
		"declarationMap": false,
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src",
		"verbatimModuleSyntax": false,
		"noEmit": true
	},
	"include": ["src"]
}
