    <bpmn:serviceTask id="Activity_04y59h5" name="Github:&#10;Add Design Link as custom Field to Project" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="8" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="bearer" target="authentication.type" />
          <zeebe:input source="{{secrets.PDP_GITHUB_PAT}}" target="authentication.token" />
          <zeebe:input source="POST" target="method" />
          <zeebe:input source="https://api.github.com/graphql" target="url" />
          <zeebe:input source="20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="20" target="readTimeoutInSeconds" />
          <zeebe:input source="={&#10;  query: &#34;mutation { updateProjectV2ItemFieldValue( input: { projectId: \&#34;&#34; + projectId + &#34;\&#34; itemId: \&#34;&#34; + projectData.epicProjectItemId + &#34;\&#34; fieldId: \&#34;&#34; + projectFieldIdDesignLink + &#34;\&#34; value: { text: \&#34;&#34; + newDesignLink + &#34;\&#34; } } ) { projectV2Item { id } } }&#34;&#10;}" target="body" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0altl3k</bpmn:incoming>
      <bpmn:outgoing>Flow_14rbsbu</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_14rbsbu" sourceRef="Activity_04y59h5" targetRef="Activity_088zr88" />
    <bpmn:serviceTask id="Activity_1uz717b" name="Slack:&#10;Notify about new Design link" zeebe:modelerTemplate="io.camunda.connectors.Slack.v1" zeebe:modelerTemplateVersion="5" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:slack:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="{{secrets.PDP_SLACK_CAMUNDA}}" target="token" />
          <zeebe:input source="chat.postMessage" target="method" />
          <zeebe:input source="messageBlock" target="data.messageType" />
          <zeebe:input source="=[&#10;	{&#10;			&#34;type&#34;: &#34;section&#34;,&#10;			&#34;text&#34;: {&#10;				&#34;type&#34;: &#34;mrkdwn&#34;,&#10;				&#34;text&#34;: &#34;A new design link was added: &#34; + newDesignLink&#10;			}&#10;		}&#10;]" target="data.blockContent" />
          <zeebe:input source="=generatedSlackChannel" target="data.channel" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1sebfr6</bpmn:incoming>
      <bpmn:outgoing>Flow_185194a</bpmn:outgoing>
