  <bpmn:process id="Process_coaepop" name="Handle PDP - Comment - Design Update" isExecutable="true">
    <bpmn:sequenceFlow id="Flow_1oomfgd" sourceRef="Activity_0onjdaz" targetRef="Activity_1hhu21g" />
    <bpmn:scriptTask id="Activity_0onjdaz" name="Get Design Link">
      <bpmn:extensionElements>
        <zeebe:script expression="=replace(split(webhook.body.comment.body, &#34; &#34;)[count(split(webhook.body.comment.body, &#34; &#34;))], &#34;&#60;|&#62;&#34;, &#34;&#34;)" resultVariable="newDesignLink" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0rk0equ</bpmn:incoming>
      <bpmn:outgoing>Flow_1oomfgd</bpmn:outgoing>
    </bpmn:scriptTask>
    <bpmn:callActivity id="Activity_1hhu21g" name="Get Project Data">
      <bpmn:extensionElements>
        <zeebe:calledElement processId="pdp-get-project-data" propagateAllChildVariables="true" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1oomfgd</bpmn:incoming>
